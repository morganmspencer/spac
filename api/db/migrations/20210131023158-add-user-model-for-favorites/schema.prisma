datasource DS {
  provider = ["sqlite", "postgresql"]
  url = "***"
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model Spac {
  id          String    @id @default(cuid())
  symbol      String    @unique
  ipoSymbol   String    @unique
  ipoDate     DateTime?
  ipoPrice    String?
  mergerDate  DateTime?
  users       User[]
  createdAt   DateTime  @default(now())
}

model User {
  id        String  @id @default(cuid())
  favorites Spac[]
}
