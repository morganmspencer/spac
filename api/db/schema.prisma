datasource DS {
  provider = ["sqlite", "postgresql"]
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model Spac {
  id          String    @id @default(cuid())
  symbol      String    @unique
  ipoSymbol   String
  ipoDate     DateTime
  ipoPrice    String?
  mergerDate  DateTime?
}
